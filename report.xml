<root>
    <base_url>http://192.168.33.63/trip/report.xml</base_url>
    <infos>
        <item>
            <name>test</name>
            <event>trip.client.ResourceManager/resource_added</event>
            <conds></conds>
            <url>http://192.168.33.63/trip/report.xml</url>
            <params>
                <item>
                    <name>A</name>
                    <value>${/event/resource/meta/segcount}</value>
                </item>
            </params>
        </item>
        <item>
            <name>test_timer</name>
            <event>trip.client.TimerManager/t_10_s</event>
            <url>http://192.168.33.63/trip/report.xml</url>
            <params>
                <item>
                    <name>A</name>
                    <value>${/udp/socket/stat/send_bytes/total}</value>
                    <procs>incremmental</procs>
                </item>
                <item>
                    <name>B</name>
                    <value>${/udp/socket/stat/recv_bytes/total}</value>
                    <procs>incremmental</procs>
                </item>
            </params>
        </item>
    </infos>
</root>
