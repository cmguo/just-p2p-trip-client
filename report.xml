<root>
    <base_url>http://192.168.33.63/trip/report.xml</base_url>
    <infos>
        <item>
            <name>test</name>
            <event>trip.client.ResourceManager/resource_added</event>
            <conds></conds>
            <url>http://192.168.33.63/trip/report.xml</url>
            <params>
                <item>
                    <name>A</name>
                    <path>/resource/@first=${/resource/id}/second/meta/segcount</path>
                </item>
            </params>
        </item>
        <item>
            <name>test_timer</name>
            <event>trip.client.TimerManager/t_10_s</event>
            <conds>
                <item>${/now} =~ \d+:\d[0,5]:0\d\.\d+</item>
            </conds>
            <url>http://192.168.33.63/trip/report.xml</url>
            <params>
                <item>
                    <name>A</name>
                    <path>/udp/socket/stat/send_bytes/total</path>
                </item>
                <item>
                    <name>B</name>
                    <path>/udp/socket/stat/recv_bytes/total</path>
                </item>
            </params>
        </item>
    </infos>
</root>
